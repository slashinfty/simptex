{
  "manifest_version": 3,
  "name": "SimpTeX",
  "version": "1.0",
  "description": "Simple LaTeX equation image generating browser extension",
  "homepage_url": "https://github.com/slashinfty/simptex",
  "icons": {
    "48": "simptex-48.png"
  },
  "action": {
    "default_icon": "simptex-32.png",
    "default_title": "SimpTeX",
    "default_popup": "simptex.html"
  }
}